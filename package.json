{
	"name": "trio",
	"version": "0.1.0",
	"repository": "https://github.com/ky-is/trio",
	"license": "ISC",
	"scripts": {
		"serve": "vue-cli-service serve",
		"start": "node src/server/index.js",
		"build": "vue-cli-service build --modern",
		"schema": "pg_dump --schema-only --no-owner --no-privileges trio | sed -e '/^--/d' -e '/^SET/d' -e '/^COMMENT ON/d' -e 's/    /\t/g' | sed '/^$/N;/^\\n$/D' > db.sql",
		"lint": "vue-cli-service lint"
	},
	"engines": {
		"node": "^10.0",
		"npm": "^6.0"
	},
	"dependencies": {
		"@sendgrid/mail": "^6.3.1",
		"bluebird": "^3.5.1",
		"dotenv": "^6.0.0",
		"express": "^4.16.3",
		"pg-promise": "^8.4.4",
		"request": "^2.88.0",
		"socket.io": "^2.1.1"
	},
	"devDependencies": {
		"@ky-is/vue-markdown-poi": "^0.2.0",
		"@vue/cli-plugin-babel": "^3.0.0",
		"@vue/cli-plugin-eslint": "^3.0.0",
		"@vue/cli-service": "^3.0.0",
		"spark-md5": "^3.0.0",
		"stylus": "^0.54.5",
		"stylus-loader": "^3.0.2",
		"vue": "^2.5.17",
		"vue-router": "^3.0.1",
		"vue-template-compiler": "^2.5.17",
		"vuex": "^3.0.1"
	},
	"babel": {
		"presets": [
			"@vue/app"
		]
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": {
		"production": [
			"> 1%",
			"last 2 versions",
			"not ie <= 8"
		],
		"development": [
			"last 1 version",
			"not dead"
		]
	}
}
